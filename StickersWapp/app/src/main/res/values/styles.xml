<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryDark">@color/primaryDarkColor</item>
        <item name="colorAccent">@color/acent_Color</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="ThemeFading">
        <item name="colorAccent">@color/acent_Color</item>
        <item name="android:color">@color/acent_Color</item>
        <item name="colorPrimary">@color/colorYuan</item>
        <item name="colorPrimaryDark">@color/colorYuan</item>
    </style>

    <style name="etiqueta">
        <item name="android:background">@drawable/marco_etiqueta</item>
        <item name="android:textColor">#000000</item>
    </style>
</resources>